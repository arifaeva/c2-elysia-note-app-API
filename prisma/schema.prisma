generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id String @id @default(cuid())
  email String @unique
  password String

  sessions Session[]
}

model Session {
  id String @id @default(cuid())
  userId String
  user User @relation(fields: [userId], references: [id])
}

model Note {
  id String @id @default(cuid())
  content String
  isDone Boolean @default(false)
}